#~ "Keyboard Shortcuts\n"
#~ "------------------\n"
#~ "? = show help\n"
#~ "j = move down\n"
#~ "k = move up\n"
#~ "J = shift row down\n"
#~ "K = shift row up\n"
#~ "\n"
#~ "1, p = pick\n"
#~ "2, r = reword\n"
#~ "3, e = edit\n"
#~ "4, f = fixup\n"
#~ "5, s = squash\n"
#~ "spacebar = toggle enabled\n"
#~ "\n"
#~ "ctrl+enter = accept changes and rebase\n"
